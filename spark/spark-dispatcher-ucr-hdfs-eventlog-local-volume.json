{
  "id": "spark-dispatcher-local-volume",
  "cpus": 2,
  "mem": 4096,
  "cmd": "/sbin/init.sh",
  "user": "nobody",
  "container": {
    "type": "MESOS",
    "docker": {
      "image": "mesosphere/spark:2.1.0-2.2.0-1-hadoop-2.7"
    },
    "volumes": [
      {
        "containerPath": "spark-dispatcher-local-volume",
        "persistent": {
          "type": "root",
          "size": 10,
          "name": "spark-dispatcher-local-volume"
          },
        "mode": "RW"
      }
    ]
  },
  "residency": {
    "taskLostBehavior": "WAIT_FOREVER"
  },
  "fetch": [
    {
      "uri": "http://api.hdfs.marathon.l4lb.thisdcos.directory/v1/endpoints/hdfs-site.xml",
      "extract": false,
      "executable": false,
      "cache": false
    },
    {
      "uri": "http://api.hdfs.marathon.l4lb.thisdcos.directory/v1/endpoints/core-site.xml",
      "extract": false,
      "executable": false,
      "cache": false
    }
  ],
  "env": {
    "DCOS_SERVICE_NAME": "spark-dispatcher-local-volume",
    "HADOOP_CONF_DIR": "/etc/hadoop",
    "JAVA_HOME": "/usr/lib/jvm/jre1.8.0_112",
    "LD_LIBRARY_PATH": "/opt/mesosphere/lib:/opt/mesosphere/libmesos-bundle/lib:/usr/lib",
    "NO_BOOTSTRAP": "true",
    "PATH": "/usr/bin:/bin:/usr/lib/jvm/jre1.8.0_112/bin:/opt/spark/dist/bin",
    "TERM": "xterm",
    "SPARK_WORKER_CORES": "2",
    "SPARK_WORKER_MEMORY": "3072",
    "SPARK_DISPATCHER_MESOS_PRINCIPAL": "",
    "SPARK_DISPATCHER_MESOS_ROLE": "*",
    "SPARK_DISPATCHER_MESOS_SECRET": "",
    "SPARK_HDFS_CONFIG_URL": "http://api.hdfs.marathon.l4lb.thisdcos.directory/v1/endpoints",
    "SPARK_LOG_LEVEL": "INFO",
    "SPARK_USER": "nobody"
  },
  "labels": {
    "MARATHON_SINGLE_INSTANCE_APP": "true",
    "DCOS_PACKAGE_DEFINITION": "",
    "DCOS_PACKAGE_FRAMEWORK_NAME": "spark-dispatcher-local-volume",
    "DCOS_PACKAGE_METADATA": "",
    "DCOS_PACKAGE_NAME": "spark",
    "DCOS_PACKAGE_OPTIONS": "e30=",
    "DCOS_PACKAGE_SOURCE": "https://universe.mesosphere.com/repo",
    "DCOS_PACKAGE_VERSION": "2.1.0-2.2.0-1",
    "DCOS_SERVICE_NAME": "spark-dispatcher-local-volume",
    "DCOS_SERVICE_PORT_INDEX": "2",
    "DCOS_SERVICE_SCHEME": "http",
    "SPARK_URI": ""
  },
  "portDefinitions": [
    {
      "port": 0,
      "protocol": "tcp",
      "name": "dispatcher",
      "labels": {
        "VIP_0": "spark-dispatcher-local-volume:7077"
      }
    },
    {
      "port": 0,
      "protocol": "tcp",
      "name": "dispatcher-ui",
      "labels": {
        "VIP_1": "spark-dispatcher-local-volume:4040"
      }
    },
    {
      "port": 0,
      "protocol": "tcp",
      "name": "dispatcher-proxy",
      "labels": {
        "VIP_2": "spark-dispatcher-local-volume:80"
      }
    }
  ],
  "healthChecks": [
    {
      "portIndex": 2,
      "protocol": "MESOS_HTTP",
      "path": "/",
      "gracePeriodSeconds": 5,
      "intervalSeconds": 60,
      "timeoutSeconds": 10,
      "maxConsecutiveFailures": 3
    }
  ],
  "upgradeStrategy": {
    "minimumHealthCapacity": 0,
    "maximumOverCapacity": 0
  }
}
