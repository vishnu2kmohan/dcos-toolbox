{
  "id": "registry",
  "cpus": 1,
  "mem": 4096,
  "instances": 1,
  "container": {
    "docker": {
      "image": "registry:2.5.1",
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 5000,
          "hostPort": 0,
          "protocol": "tcp",
          "name": "registry",
          "labels": {
            "VIP_0": "/registry:5000"
          }
        }
      ]
    },
    "type": "DOCKER",
    "volumes": [
      {
        "containerPath": "var",
        "mode": "RW",
        "persistent": {
          "size": 10240
        }
      }
    ]
  },
  "env": {
    "STORAGE_PATH": "/var/lib/registry"
  },
  "labels": {
    "DCOS_PACKAGE_IS_FRAMEWORK": "false",
    "DCOS_PACKAGE_METADATA": "",
    "DCOS_PACKAGE_NAME": "registry",
    "DCOS_PACKAGE_REGISTRY_VERSION": "3.0",
    "DCOS_PACKAGE_RELEASE": "0",
    "DCOS_PACKAGE_SOURCE": "https://universe.mesosphere.com/repo",
    "DCOS_PACKAGE_VERSION": "5.7.12-0.3",
    "DCOS_SERVICE_NAME": "registry",
    "MARATHON_SINGLE_INSTANCE_APP": "true"
  },
  "upgradeStrategy": {
    "maximumOverCapacity": 0,
    "minimumHealthCapacity": 0
  },
  "healthChecks": [
    {
      "portIndex": 0,
      "protocol": "MESOS_TCP",
      "gracePeriodSeconds": 300,
      "intervalSeconds": 60,
      "timeoutSeconds": 20,
      "maxConsecutiveFailures": 3
    }
  ]
}
